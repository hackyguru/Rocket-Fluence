{
  "name": "IPFS Man",
  "description": "Connecting to the future of internet - Web3 through IPFS",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Press to open"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxr...'; object-src 'self'",
 

  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "background":{
    "scripts": [
      "menu.js"
    ],
    "persistent":false
  }
 
}